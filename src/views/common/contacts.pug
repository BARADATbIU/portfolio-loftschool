.contacts
  .contacts__title
    +subtitle('contacts', 'Контакты')
  ul.contacts__list
    each item in contacts
      li.contacts__item
        a.contacts__link(href=`${item.link}` target="_blank")
          +icon(item.img, 'contacts__img', '24px', '24px')
          .contacts__text #{item.text}