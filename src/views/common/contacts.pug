  var contacts = [
    {
      img: 'skype',
      text: 'dimaharlamov',
      link: 'tel:dimaharlamov'
    },{
      img: 'envelope',
      text: 'newdimaak@gmail.com',
      link: 'mailto:newdimaak@gmail.com'
    },{
      img: 'phone',
      text: '+375 29 556 35 00',
      link: 'tel:+755555555'
    }
  ]

.contacts
  .contacts__title
    +subtitle('contacts', 'Контакты')
  ul.contacts__list
    each item in contacts
      li.contacts__item
        a.contacts__link(href=`${item.link}` target="_blank")
          +icon(item.img, 'contacts__img', '1.5rem', '1.5rem')
          .contacts__text #{item.text}